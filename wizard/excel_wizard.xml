<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="view_excel_report_wizard" model="ir.ui.view">
        <field name="name">charging.excel.wizard.form</field>
        <field name="model">charging.excel.wizard</field>
        <field name="arch" type="xml">
            <form string="Excel Report">
                <group>
                    <group>
                        <field name="start_date" required="1"/>
                    </group>
                    <group> 
                        <field name="end_date" required="1"/>
                    </group>
                </group>
                <footer>
                    <button name="print_xls_report" string="Export" type="object" class="btn-primary"/>
                    <button string="Cancel" class="btn-default" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>


</odoo>